{
  "name": "@langnang/gh-api",
  "description": "GitHub API",
  "version": "0.0.2",
  "main": "lib/gh-api.umd.min.js",
  "private": false,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --name gh-api --dest lib packages/index.js",
    "build:publish": "npm run lib && npm publish --access publish"
  },
  "dependencies": {
    "axios": "^0.21.0"
  },
  "devDependencies": {},
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "author": "langnang",
  "license": "ISC",
  "directories": {
    "example": "examples",
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/langnang/gh-api.git"
  },
  "keywords": [
    "langnang",
    "github",
    "gh",
    "api"
  ],
  "bugs": {
    "url": "https://github.com/langnang/gh-api/issues"
  },
  "homepage": "https://github.com/langnang/gh-api#readme"
}